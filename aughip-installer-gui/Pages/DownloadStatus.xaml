<UserControl
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:aughip_installer_gui.Pages"
        xmlns:res="clr-namespace:aughip_installer_gui.Properties;assembly=aughip-installer-gui"
        xmlns:Controls="clr-namespace:aughip_installer_gui.Controls" x:Class="aughip_installer_gui.Pages.DownloadStatus"
        mc:Ignorable="d"
        d:DesignHeight="471" d:DesignWidth="800">
    <DockPanel Background="{StaticResource BrushPrimary}" Height="471" Margin="0">

        <StackPanel>
            <Controls:DownloadItem x:Name="vcRedistProgress" ProgressValue="0" Text="{x:Static res:Resources.DOWNLOAD_MODULE_VCREDIST}" VerticalAlignment="Top"/>
            <Controls:DownloadItem x:Name="augHipProgress" ProgressValue="0" Text="{x:Static res:Resources.DOWNLOAD_MODULE_AUGHIP}" VerticalAlignment="Top"/>
        </StackPanel>

    </DockPanel>
</UserControl>
